<div class="page-body petxpert-new-background">

    <h1 class="page-title">Create a PetXpert Profile</h1>

    <% if @petxpert.errors %>
    <ul class = "error_list" %>
        <% @petxpert.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
    </ul>
    <% end %>

    <%= render "form" %>
</div>