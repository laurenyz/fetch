<div class="page-body services-background">
    <h1 class="page-title">Add a Service</h1>

    <% if @service.errors %>
    <ul class = "error_list" %>
        <% @service.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
    </ul>
    <% end %>

    <%= render "form" %>
</div>